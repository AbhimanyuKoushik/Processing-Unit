:vpi_module "/usr/lib/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/ivl/va_math.vpi";
S_0x55f4d9c030e0 .scope module, "divide_tb" "divide_tb" 2 3;
 .timescale -9 -12;
v0x55f4d9c18560_0 .var "flag", 1 0;
v0x55f4d9c18640_0 .var "in1", 63 0;
v0x55f4d9c18710_0 .var "in2", 63 0;
v0x55f4d9c18810_0 .net "out", 63 0, v0x55f4d9c18400_0;  1 drivers
S_0x55f4d9c03270 .scope module, "dut" "divide" 2 9, 3 1 0, S_0x55f4d9c030e0;
 .timescale 0 0;
    .port_info 0 /INPUT 64 "in1";
    .port_info 1 /INPUT 64 "in2";
    .port_info 2 /INPUT 2 "flag";
    .port_info 3 /OUTPUT 64 "out";
v0x55f4d9c034c0_0 .net "flag", 1 0, v0x55f4d9c18560_0;  1 drivers
v0x55f4d9c18260_0 .net "in1", 63 0, v0x55f4d9c18640_0;  1 drivers
v0x55f4d9c18340_0 .net "in2", 63 0, v0x55f4d9c18710_0;  1 drivers
v0x55f4d9c18400_0 .var "out", 63 0;
E_0x55f4d9c015f0 .event anyedge, v0x55f4d9c034c0_0, v0x55f4d9c18260_0, v0x55f4d9c18340_0;
    .scope S_0x55f4d9c03270;
T_0 ;
    %wait E_0x55f4d9c015f0;
    %load/vec4 v0x55f4d9c034c0_0;
    %dup/vec4;
    %pushi/vec4 0, 0, 2;
    %cmp/u;
    %jmp/1 T_0.0, 6;
    %dup/vec4;
    %pushi/vec4 1, 0, 2;
    %cmp/u;
    %jmp/1 T_0.1, 6;
    %dup/vec4;
    %pushi/vec4 2, 0, 2;
    %cmp/u;
    %jmp/1 T_0.2, 6;
    %load/vec4 v0x55f4d9c18260_0;
    %load/vec4 v0x55f4d9c18340_0;
    %mod;
    %store/vec4 v0x55f4d9c18400_0, 0, 64;
    %jmp T_0.4;
T_0.0 ;
    %load/vec4 v0x55f4d9c18260_0;
    %load/vec4 v0x55f4d9c18340_0;
    %div;
    %store/vec4 v0x55f4d9c18400_0, 0, 64;
    %jmp T_0.4;
T_0.1 ;
    %load/vec4 v0x55f4d9c18260_0;
    %load/vec4 v0x55f4d9c18340_0;
    %div;
    %store/vec4 v0x55f4d9c18400_0, 0, 64;
    %jmp T_0.4;
T_0.2 ;
    %load/vec4 v0x55f4d9c18260_0;
    %load/vec4 v0x55f4d9c18340_0;
    %mod;
    %store/vec4 v0x55f4d9c18400_0, 0, 64;
    %jmp T_0.4;
T_0.4 ;
    %pop/vec4 1;
    %jmp T_0;
    .thread T_0, $push;
    .scope S_0x55f4d9c030e0;
T_1 ;
    %vpi_call 2 17 "$display", "Time\011flag\011in1\011\011\011in2\011\011\011out" {0 0 0};
    %vpi_call 2 18 "$monitor", "%0t\011%b\011%h\011%h\011%h", $time, v0x55f4d9c18560_0, v0x55f4d9c18640_0, v0x55f4d9c18710_0, v0x55f4d9c18810_0 {0 0 0};
    %pushi/vec4 10, 0, 64;
    %store/vec4 v0x55f4d9c18640_0, 0, 64;
    %pushi/vec4 5, 0, 64;
    %store/vec4 v0x55f4d9c18710_0, 0, 64;
    %pushi/vec4 0, 0, 2;
    %store/vec4 v0x55f4d9c18560_0, 0, 2;
    %delay 10000, 0;
    %pushi/vec4 20, 0, 64;
    %store/vec4 v0x55f4d9c18640_0, 0, 64;
    %pushi/vec4 4294967295, 0, 32;
    %concati/vec4 4294967289, 0, 32;
    %store/vec4 v0x55f4d9c18710_0, 0, 64;
    %pushi/vec4 1, 0, 2;
    %store/vec4 v0x55f4d9c18560_0, 0, 2;
    %delay 10000, 0;
    %pushi/vec4 100, 0, 64;
    %store/vec4 v0x55f4d9c18640_0, 0, 64;
    %pushi/vec4 20, 0, 64;
    %store/vec4 v0x55f4d9c18710_0, 0, 64;
    %pushi/vec4 2, 0, 2;
    %store/vec4 v0x55f4d9c18560_0, 0, 2;
    %delay 10000, 0;
    %pushi/vec4 5, 0, 64;
    %store/vec4 v0x55f4d9c18640_0, 0, 64;
    %pushi/vec4 4294967295, 0, 32;
    %concati/vec4 4294967294, 0, 32;
    %store/vec4 v0x55f4d9c18710_0, 0, 64;
    %pushi/vec4 3, 0, 2;
    %store/vec4 v0x55f4d9c18560_0, 0, 2;
    %delay 10000, 0;
    %vpi_call 2 32 "$finish" {0 0 0};
    %end;
    .thread T_1;
