S_0x5607830ff200 .scope module, "add_sub_tb" "add_sub_tb" 2 3;
 .timescale -9 -12;
v0x560783114b90_0 .var "flag", 1 0;
v0x560783114c70_0 .var "in1", 63 0;
v0x560783114d40_0 .var "in2", 63 0;
v0x560783114e40_0 .net "out", 63 0, v0x560783114a30_0;  1 drivers
S_0x5607830ff390 .scope module, "dut" "add_sub" 2 9, 3 7 0, S_0x5607830ff200;
 .timescale 0 0;
    .port_info 0 /INPUT 64 "in1";
    .port_info 1 /INPUT 64 "in2";
    .port_info 2 /INPUT 2 "flag";
    .port_info 3 /OUTPUT 64 "out";
v0x5607830ff5e0_0 .net "flag", 1 0, v0x560783114b90_0;  1 drivers
v0x560783114890_0 .net "in1", 63 0, v0x560783114c70_0;  1 drivers
v0x560783114970_0 .net "in2", 63 0, v0x560783114d40_0;  1 drivers
v0x560783114a30_0 .var "out", 63 0;
E_0x5607830fd5f0 .event anyedge, v0x5607830ff5e0_0, v0x560783114890_0, v0x560783114970_0;
    .scope S_0x5607830ff390;
T_0 ;
    %wait E_0x5607830fd5f0;
    %load/vec4 v0x5607830ff5e0_0;
    %dup/vec4;
    %pushi/vec4 0, 0, 2;
    %cmp/u;
    %jmp/1 T_0.0, 6;
    %dup/vec4;
    %pushi/vec4 1, 0, 2;
    %cmp/u;
    %jmp/1 T_0.1, 6;
    %dup/vec4;
    %pushi/vec4 2, 0, 2;
    %cmp/u;
    %jmp/1 T_0.2, 6;
    %load/vec4 v0x560783114890_0;
    %load/vec4 v0x560783114970_0;
    %cmp/u;
    %flag_mov 8, 5;
    %jmp/0 T_0.5, 8;
    %pushi/vec4 1, 0, 64;
    %jmp/1 T_0.6, 8;
T_0.5 ; End of true expr.
    %pushi/vec4 0, 0, 64;
    %jmp/0 T_0.6, 8;
 ; End of false expr.
    %blend;
T_0.6;
    %store/vec4 v0x560783114a30_0, 0, 64;
    %jmp T_0.4;
T_0.0 ;
    %load/vec4 v0x560783114890_0;
    %load/vec4 v0x560783114970_0;
    %add;
    %store/vec4 v0x560783114a30_0, 0, 64;
    %jmp T_0.4;
T_0.1 ;
    %load/vec4 v0x560783114890_0;
    %load/vec4 v0x560783114970_0;
    %sub;
    %store/vec4 v0x560783114a30_0, 0, 64;
    %jmp T_0.4;
T_0.2 ;
    %load/vec4 v0x560783114890_0;
    %load/vec4 v0x560783114970_0;
    %cmp/u;
    %flag_mov 8, 5;
    %jmp/0 T_0.7, 8;
    %pushi/vec4 1, 0, 64;
    %jmp/1 T_0.8, 8;
T_0.7 ; End of true expr.
    %pushi/vec4 0, 0, 64;
    %jmp/0 T_0.8, 8;
 ; End of false expr.
    %blend;
T_0.8;
    %store/vec4 v0x560783114a30_0, 0, 64;
    %jmp T_0.4;
T_0.4 ;
    %pop/vec4 1;
    %jmp T_0;
    .thread T_0, $push;
    .scope S_0x5607830ff200;
T_1 ;
    %vpi_call 2 17 "$display", "Time\011flag\011in1\011\011\011in2\011\011\011out" {0 0 0};
    %vpi_call 2 18 "$monitor", "%0t\011%b\011%h\011%h\011%h", $time, v0x560783114b90_0, v0x560783114c70_0, v0x560783114d40_0, v0x560783114e40_0 {0 0 0};
    %pushi/vec4 10, 0, 64;
    %store/vec4 v0x560783114c70_0, 0, 64;
    %pushi/vec4 5, 0, 64;
    %store/vec4 v0x560783114d40_0, 0, 64;
    %pushi/vec4 0, 0, 2;
    %store/vec4 v0x560783114b90_0, 0, 2;
    %delay 10000, 0;
    %pushi/vec4 20, 0, 64;
    %store/vec4 v0x560783114c70_0, 0, 64;
    %pushi/vec4 7, 0, 64;
    %store/vec4 v0x560783114d40_0, 0, 64;
    %pushi/vec4 1, 0, 2;
    %store/vec4 v0x560783114b90_0, 0, 2;
    %delay 10000, 0;
    %pushi/vec4 100, 0, 64;
    %store/vec4 v0x560783114c70_0, 0, 64;
    %pushi/vec4 200, 0, 64;
    %store/vec4 v0x560783114d40_0, 0, 64;
    %pushi/vec4 2, 0, 2;
    %store/vec4 v0x560783114b90_0, 0, 2;
    %delay 10000, 0;
    %pushi/vec4 4294967295, 0, 32;
    %concati/vec4 4294967291, 0, 32;
    %store/vec4 v0x560783114c70_0, 0, 64;
    %pushi/vec4 9, 0, 64;
    %store/vec4 v0x560783114d40_0, 0, 64;
    %pushi/vec4 3, 0, 2;
